{
  "manifest_version": 3,
  "name": "ERP Auto Login Extension",
  "version": "1.0",
  "permissions": ["storage", "activeTab", "scripting", "tabs"],
  "background": {
    "service_worker": "background.js"
  },

  "content_scripts": [
    {
      "matches": [
        "https://erp.iitkgp.ac.in/SSOAdministration/login.htm?sessionToken=6A59F5C0546AB26A81800FC8CC2F7FE0.worker3&requestedUrl=https://erp.iitkgp.ac.in/IIT_ERP3/"
      ],
      "js": ["content.js"]
    }
  ],


  "web_accessible_resources": [
    {
      "resources": ["form.html"],
      "matches": ["<all_urls>"]
    }
  ]
}
